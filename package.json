{
  "name": "nestjs-context-logger",
  "version": "1.0.1",
  "description": "nestjs-context-logger",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CONTRIBUTING.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --testPathIgnorePatterns=.*\\.live\\.",
    "test:debug": "jest debug --runInBand --detectOpenHandles",
    "test:e2e": "jest context-logger.e2e.spec.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "nestjs",
    "logger",
    "logging",
    "context",
    "pino"
  ],
  "author": {
    "name": "AdirD",
    "url": "https://github.com/AdirD"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdirD/nestjs-context-logger.git"
  },
  "bugs": {
    "url": "https://github.com/AdirD/nestjs-context-logger/issues"
  },
  "homepage": "https://github.com/AdirD/nestjs-context-logger#readme",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "nestjs-pino": ">=3.5.0",
    "pino": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.4.12",
    "@nestjs/core": "^10.4.12",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-fastify": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.9",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.18.2",
    "fastify": "^4.0.0",
    "jest": "^29.7.0",
    "pino": "^8.0.0",
    "prettier": "^3.4.1",
    "rimraf": "^5.0.0",
    "rxjs": "^7.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "uuid": "^9.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
